<eihwaz.de>
<append xpath="/lootcontainers/lootqualitytemplates">
	<lootqualitytemplate name="default_qualityT">
		<qualitytemplate level="1,999999">
			<loot quality="1" prob="0.05"/>
			<loot quality="2" prob="0.1"/>
			<loot quality="3" prob="0.2"/>
			<loot quality="4" prob="0.3"/>
			<loot quality="5" prob="0.2"/>
			<loot quality="6" prob="0.1"/>
		</qualitytemplate>
	</lootqualitytemplate>
</append>
	
<append xpath="/lootcontainers/lootgroup[@name='groupQuestWeaponsRangedT3']"><item name="Savery"/></append>
<append xpath="/lootcontainers/lootgroup[@name='groupQuestAmmo']">
	<item name="Energy Cells" count="64" loot_prob_template="QuestT3Prob"/>
	<item name="Gamma Round" count="64" loot_prob_template="QuestT3Prob"/>
	<item name="RailgunAmmo" count="64" loot_prob_template="QuestT3Prob"/>
	<item name="arramus Fire Round" count="64" loot_prob_template="QuestT3Prob"/>
</append>
	
<append xpath="/lootcontainers">
	<lootgroup name="goldLoot" loot_quality_template="default_qualityT">
		<item name="Gamma Gun" prob=".17"/>
		<item name="Railgun" prob=".17"/>
		<item name="Newcomen" prob=".17"/>
		<item name="CrimsonHunter" prob=".17"/>
		<item name="HyperBlaster" prob=".17"/>
		<item name="Vulcan" prob=".17"/>
		<item name="Vampire Gauntlets" prob=".17"/>
		<item name="turretAuger" prob=".17"/>
		<item name="Kronos XII" prob=".17"/>
		<item name="Thor Hammer" prob=".17"/>
		<item name="CrossbowMag" prob=".17"/>
		<item name="Black Widow" prob=".17"/>
		<item name="vehicleHellBeastPlaceable" prob=".17"/>
		<item name="vehicleHellFirePlaceable" prob=".17"/>
		<item name="vehicleHellDogPlaceable" prob=".17"/>
		<item name="AutoMiner" prob=".17"/>
		<item name="apparelNightvisionGoggles" prob=".17"/>
		<item name="modGunFlashlight" prob=".17"/>
	</lootgroup>
</append>
</eihwaz.de>