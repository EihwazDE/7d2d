<!-- Boar Spawner -->
<!--place-->
<append xpath="/blocks">
<block name="h7sb_boarSpawnerPlace">
	<property name="CreativeMode" value="None"/>
    <property name="Material" value="Mbedrock"/>
    <property name="Shape" value="Invisible"/>
	<property name="Texture" value="250"/>
    <property name="Collide" value="movement,melee,bullet,arrow,rocket"/>
	<property name="Place" value="TowardsPlacer"/>
	<property name="OnlySimpleRotations" value="true"/>
	<property name="IsTerrainDecoration" value="true"/>
	<property name="SellableToTrader" value="false"/>
	<property name="Class" value="PlantGrowing"/>
	<property name="PlantGrowing.FertileLevel" value="0"/>
	<property name="PlantGrowing.Next" value="h7sb_boarSpawnerRemove"/>
	<property name="PlantGrowing.GrowthRate" value=".001"/>
	<property name="PlantGrowing.IsRandom" value="false"/>
	<property name="PlantGrowing.LightLevelStay" value="0"/>
	<property name="PlantGrowing.LightLevelGrow" value="0"/>
	<property name="PlantGrowing.GrowOnTop" value="h7sb_boarSpawner"/>
	<property name="PlantGrowing.IsGrowOnTopEnabled" value="true"/>
	<drop event="Destroy" count="0"/>
</block>
</append>
<!--remove-->
<append xpath="/blocks">
<block name="h7sb_boarSpawnerRemove">
	<property name="CreativeMode" value="None"/>
	<property name="Class" value="Mine"/>
	<property name="Material" value="Mbedrock"/>
	<property name="StabilitySupport" value="true"/>
	<property name="Shape" value="Invisible"/>
	<property name="Texture" value="250"/>
	<property name="IsTerrainDecoration" value="true"/>
	<property name="Collide" value="movement,melee,bullet,arrow,rocket"/>
	<property name="SellableToTrader" value="false"/>
	<property name="TriggerDelay" value="0.5"/>
	<property name="TriggerSound" value="landmine_trigger"/>
	<property name="Explosion.ParticleIndex" value="0"/>
	<property name="Explosion.RadiusBlocks" value="1"/>
	<property name="Explosion.RadiusEntities" value="1"/>
	<property name="Explosion.BlockDamage" value="1"/>
	<property name="Explosion.EntityDamage" value="0"/>
	<property name="CanPickup" value="false"/>
	<drop event="Destroy" count="0"/>
</block>
</append>
